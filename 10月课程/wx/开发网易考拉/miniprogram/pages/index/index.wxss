@keyframes mymove
	{
25% {background-color:rgb(248, 144, 9);font-size: 8px;}
50%{background-color:rgba(248, 234, 35, 0.973);font-size:18px;}
75%{background-color:rgba(248, 87, 248, 0.973);font-size:33px;}
100%{background-color:rgba(245, 165, 227, 0.973);font-size:14px;}
}
